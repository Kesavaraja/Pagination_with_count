{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": false
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    }, /* 
    "babelOptions": {
        "presets": [
            "@babel/preset-react"
        ]
    }, */
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "react/prop-types": "off"
    }
}